<?xml version="1.0"?>
<?xml-model
  href="http://download.ros.org/schema/package_format2.xsd"
  schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
  <name>prometheus_cpp_vendor</name>
  <version>0.1.0</version>
  <description>
    Wrapper around jupp0r/prometheum-cpp.
  </description>

  <maintainer email="wjwwood@intrinsic.ai">William Woodall</maintainer>

  <license>Apache-2.0</license>
  <license>MIT</license>  <!-- The contents of prometheus-cpp are MIT -->

  <author email="wjwwood@intrinsic.ai">William Woodall</author>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_vendor_package</buildtool_depend>

  <build_depend>libcurl-dev</build_depend>
  <build_export_depend>libcurl-dev</build_export_depend>
  <exec_depend>libcurl</exec_depend>

  <depend>abseil_cpp_vendor</depend>
  <depend>zlib</depend>

  <export>
    <build_type>ament_cmake</build_type>
